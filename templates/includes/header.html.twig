                <li><a href="{{ path('cart') }}">Panier</a></li>
                <li><a href="{{ path('logout') }}">Se d√©connecter</a></li>
               
            {% else %}
                <li><a href="{{ path('home') }}">Accueil</a></li>
                <li><a href="{{ path('login') }}">Se connecter</a></li>
                <li><a href="{{ path('registration') }}">S'inscrire</a></li>
            {% endif %}
        </ul
    </nav>
</header>

</body>
</html>
